    "if len(window_arr)==len(set(window_arr)):\n",
    "        max_ans = max(sum(window_arr),max_ans) \n",
    "\n",
    "def check_reply(arr):\n",
    "    for e in set(arr):\n",
    "        if arr.count(e)>1:\n",
    "            return 0\n",
    "    return 1\n",
